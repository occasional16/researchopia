{
  "name": "semiff",
  "version": "2.0.0",
  "description": "A tiny (740B) utility to get the type difference between two semver versions.",
  "type": "commonjs",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "./semiff.d.ts",
  "sideEffects": false,
  "exports": {
    ".": {
      "import": {
        "types": "./semiff.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./semiff.d.ts",
        "default": "./dist/index.js"
      }
    }
  },
  "repository": "mrozio13pl/semiff",
  "author": "mrozio13pl",
  "license": "MIT",
  "files": [
    "dist",
    "semiff.d.ts",
    "semiff.d.mts"
  ],
  "keywords": [
    "semver",
    "diff"
  ]
}
